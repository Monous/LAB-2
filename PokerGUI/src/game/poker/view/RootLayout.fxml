<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane maxHeight="750.0" minHeight="750.0" prefHeight="750.0" prefWidth="1024.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.poker.RootController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Game">
              <items>
                <MenuItem fx:id="fiveCardJokerPoker" mnemonicParsing="false" text="5 Card Joker Poker" />
                  <MenuItem fx:id="fiveCardWildPoker" mnemonicParsing="false" text="5 Card Wild Poker (Deuces Wild)" />
                  <MenuItem fx:id="fiveCardDraw" mnemonicParsing="false" text="5 Card Draw" />
                  <MenuItem fx:id="sevenCardDraw" mnemonicParsing="false" text="7 Card Draw" />
                  <MenuItem fx:id="texasHoldEm" mnemonicParsing="false" text="Texas Hold 'em " />
                  <MenuItem fx:id="omaha" mnemonicParsing="false" text="Omaha" />
                  <MenuItem fx:id="fiveCardStud" mnemonicParsing="false" text="5 Card Stud" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
